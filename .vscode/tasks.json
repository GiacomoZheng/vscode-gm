{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"type": "npm",
			"script": "install",
			"problemMatcher": []
		},
		{
            "label": "compile and copy",
            "type": "shell",
            "command": "Set-Location \"C:\\Program Files\\my soft\\AutoHotkey\\Compiler\\\";.\\Ahk2Exe.exe /in C:\\Users\\admin\\Documents\\GitHub\\vscode-gm\\plug-ins\\gm.ahk;Set-Location C:\\Users\\admin\\Documents\\GitHub\\vscode-gm\\plug-ins\\;Copy-Item -Force .\\gm.exe \"C:\\Users\\admin\\AppData\\Roaming\\Microsoft\\Windows\\Start Menu\\Programs\\Startup\";",
            "group": "build",
            "presentation": {
                // Reveal the output only if unrecognized errors occur.
                "reveal": "silent"
            },
            // Use the standard MS compiler pattern to detect errors, warnings and infos
            "problemMatcher": "$msCompile"
        }
	]
}