{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "PStree",
	"scopeName": "source.pstree",
	"patterns": [
		{
			"match": "^[^├│└].+",
			"name": "comment.pstree"
		},
		{
			"match": "^((?:│  )*\\s*[├└]─)(.+)\\b",
			"captures": {
				"1": {
					"name": "keyword.brunch.pstree"
				},
				"2": {
					"name": "meta.object.pstree"
				}
			}
		}
	],

	"repsository": {}
}
