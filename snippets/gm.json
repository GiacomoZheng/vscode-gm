{
	"∈": {
		"prefix": "belong",
		"body" : [
			"∈ ${1:class/collection},$0"
		]
	},
	"⊆": {
		"prefix": "includes",
		"body" : [
			"⊆ ${1:class/collection},$0"
		]
	},
	"∀": {
		"prefix": "for_all",
		"body": [
			"∀${1:variable_name}:$0"
		]
	},
	"∃": {
		"prefix": "there_exist",
		"body": [
			"∃${1:variable_name}:$0"
		]
	},
	"≥": {
		"prefix": "bigger_or_equal_to",
		"body": ["≥"]
	},
	"≤": {
		"prefix": "smaller_or_equal_to",
		"body": ["≤"]
	},
	"⇒": {
		"prefix": "then",
		"body": ["⇒"]
	},
	"⇔": {
		"prefix": "if_and_only_if",
		"body": ["⇔"]
	},
	"∪": {
		"prefix": "union",
		"body": ["∪"]
	},
	"∩": {
		"prefix": "intersection",
		"body": ["∩"]
	},
	"■": {
		"prefix": "terminator",
		"body": ["■"]
	},

	"locator to the headers": {
		"prefix": "@gm.h",
		"body": [
			"@gm.h.$1,",
			"$0",
			"■"
		]
	},
	"variable with ∀": {
		"prefix": "∀",
		"body": [
			"∀${1:variable_name}:$0"
		]
	},
	"variable with ∃": {
		"prefix": "∃",
		"body": [
			"∃${1:variable_name}:$0"
		]
	},
	"variable with !": {
		"prefix": "!",
		"body": [
			"!${1:variable_name}:$0"
		]
	},
	"absolute value": {
		"prefix": "|",
		"body": [
			"|$1|$0"
		]
	},

	"∅": {
		"prefix": "empty_set",
		"body": ["∅"]
	},
	"⇐": {
		"prefix": "only_if",
		"body": ["⇐"]
	},
	"∋": {
		"prefix": "contain",
		"body" : [
			"∈ ${1:class/collection},$0"
		]
	},
	"⊇": {
		"prefix": "included",
		"body" : [
			"⊇ ${1:class/collection},$0"
		]
	},
	"⊃": {
		"prefix": "properly_includes",
		"body" : [
			"⊃ ${1:class/collection},$0"
		]
	},
	"⊂": {
		"prefix": "properly_included",
		"body" : [
			"⊂ ${1:class/collection},$0"
		]
	}
}